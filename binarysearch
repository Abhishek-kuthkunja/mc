AREA TEST,CODE,READONLY
ENTRY
	LDR R1,=SOURCE
	LDR R2,=STRING
	BL LOOP3
LOOP1
	LDRB R3,[R2]
	LDRB R5,[R1],#1
	CMP R5,#0
	BEQ GO
	CMP R5,R3
	BNE LOOP1
	MOV R8,R4
	MOV R6,R2
	MOV R7,R1
	SUB R8,#1
LOOP2
	LDRB R9,[R6,#1]!
	LDRB R10,[R7],#1
	CMP R9,R10
	BNE LOOP1
	SUBS R8,#1
	BNE LOOP2
	ADD R11,#1
	B LOOP1
GO B GO
LOOP3
	LDRB R3,[R2],#1
	CMP R3,#0
	ADDNE R4,#1
	BNE LOOP3
	SUB R2,R4
	SUB R2,#1
	BX LR
SOURCE DCB "ABHISHEK ABHI ABHI ABHI",0
STRING DCB "ABHI",0
	END
