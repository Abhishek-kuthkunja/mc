	AREA TEST,CODE,READONLY
ENTRY
	LDR R1,=TABLE
	LDR R2,=KEY
	MOV R3,#0
	MOV R4,#4
	MOV R10,#4
	LDR R8,[R2]
LOOP
	CMP R3,R0
	MOVGT R4,#-1
	BGT GO
	
	ADD R5,R0,R4
	LSRS R5,#1
	MUL R6,R5,R10
	ADD R7,R3,R6
	LDR R9,[R7]
	CMP R8,R9
	BEQ GO
	SUBGT R4,R5,#1
	ADDGT R3,R5,#1
GO B GO
TABLE DCD 2,4,6,8,10
KEY DCD 4
	END
